        <AUTOSTART>FALSE</AUTOSTART>
    </TASK>

    <TASK>
        <name>H4</name>
        <TYPE>BASIC</TYPE>
        <SCHEDULE>FULL</SCHEDULE>
        <PRIORITY>2</PRIORITY>
