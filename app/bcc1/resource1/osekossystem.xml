<?xml version="1.0"?>

<oseksystem xmlns              = "http://www4.informatik.uni-erlangen.de/RTSC"
            xmlns:xsi          = "http://www.w3.org/2001/XMLSchema-instance">

    <OS>
        <name>TestSystem</name>
        <STATUS>STANDARD</STATUS>
